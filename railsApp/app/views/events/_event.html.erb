<div id="<%= dom_id event %>">
  <p>
    <strong>Title:</strong>
    <%= event.title %>
  </p>

  <p>
    <strong>Allday:</strong>
    <%= event.allday %>
  </p>

  <p>
    <strong>Eventstart:</strong>
    <%= event.eventstart %>
  </p>

  <p>
    <strong>Eventend:</strong>
    <%= event.eventend %>
  </p>

  <p>
    <strong>Occurence:</strong>
    <%= event.occurence %>
  </p>

  <p>
    <strong>Details:</strong>
    <%= event.details %>
  </p>

  <p>
    <strong>Features:</strong>
    <%= event.featured %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= image_tag(event.photo, width: "50%") %>
  </p>

</div>

<div id="<%= dom_id photo %>">
  <p>
    <strong>Album:</strong>
    <%= photo.album_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= photo.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= photo.description %>
  </p>

  <p>
    <strong>Photo:</strong>
    <%= image_tag(photo.image, width: "50%") %>
  </p>

</div>
